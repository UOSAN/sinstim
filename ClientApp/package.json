{
  "name": "sin-stim",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^10.14.0"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "axios-retry": "^3.1.2",
    "bulma": "^0.7.5",
    "immer": "^3.3.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.3",
    "react": "^16.9.0",
    "react-csv": "^1.1.1",
    "react-dom": "^16.9.0",
    "react-graceful-image": "^1.2.11",
    "react-redux": "^7.1.1",
    "react-toastify": "^5.3.2",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "webpack --config ./config/webpack/webpack.config.js --progress --profile",
    "watch": "webpack --config ./config/webpack/webpack.config.js --progress --profile --watch",
    "production": "webpack --config ./config/webpack/webpack.config.js --env.production",
    "lint": "eslint --ext js,jsx src/",
    "copy-pictures-prod": "ncp ./../sample-pictures-prod/ ./../wwwroot/pictures/",
    "copy-pictures-local": "ncp ./../sample-pictures-local/ ./../wwwroot/pictures/",
    "generate-sql-script": "node ./populatePictureTable.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^3.0.1",
    "glob": "^7.1.4",
    "jest": "^24.9.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.4.1",
    "uuid": "^3.3.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.(js|jsx)"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/report-index.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/enzyme.js"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(js|jsx)"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/.+\\.(js|jsx)$"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ],
      [
        "@babel/plugin-transform-runtime"
      ]
    ]
  }
}
