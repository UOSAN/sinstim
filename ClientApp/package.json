{
  "name": "sin-stim",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^10.14.0"
  },
  "dependencies": {
    "axios": "0.18.0",
    "bulma": "0.7.4",
    "immer": "2.1.1",
    "lodash": "4.17.11",
    "prop-types": "15.7.2",
    "query-string": "6.3.0",
    "react": "16.8.4",
    "react-csv": "1.1.1",
    "react-dom": "16.8.4",
    "react-redux": "6.0.1",
    "react-toastify": "4.5.2",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "webpack --config ./config/webpack/webpack.config.js --progress --profile",
    "watch": "webpack --config ./config/webpack/webpack.config.js --progress --profile --watch",
    "production": "webpack --config ./config/webpack/webpack.config.js --env.production",
    "lint": "eslint --ext js,jsx src/",
    "copy-pictures": "ncp ./../SamplePictures/ ./../wwwroot/pictures/",
    "generate-picture-sql-script": "node ./populatePictureTable.js"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/plugin-transform-runtime": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "compression-webpack-plugin": "2.0.0",
    "css-loader": "2.1.1",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.10.0",
    "eslint": "5.15.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "jest": "24.3.1",
    "ncp": "2.0.0",
    "node-sass": "4.11.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.3",
    "uuid": "3.3.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.(js|jsx)"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/report-index.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/enzyme.js"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(js|jsx)"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/.+\\.(js|jsx)$"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ],
      [
        "@babel/plugin-transform-runtime"
      ]
    ]
  }
}
